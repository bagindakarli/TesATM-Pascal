uses crt;
var
        uang,satus,seket,sepuluh,sisa1,sisa2,sisa3:longint;
Begin
        clrscr;
        write('MASUKKAN UANG YANG AKAN DITARIK : ');
        readln(uang);
        satus:=uang div 100000;
        sisa1:=uang mod 100000;
        seket:=sisa1 div 50000;
        sisa2:=sisa1 mod 50000;
        sepuluh:=sisa2 div 10000;
        sisa3:=sisa2 mod 10000;
        clrscr;
        writeln('ANDA MENDAPATKAN : ');
        writeln(satus,' Lembar Rp100.000,-');
        writeln(seket,' Lembar Rp50.000,-');
        writeln(sepuluh,' Lembar Rp10.000,-');
        readln;
End.